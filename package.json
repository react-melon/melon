{
  "name": "melon",
  "version": "0.5.15",
  "description": "ui components for react",
  "main": "./src",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config tools/webpack.dev.js",
    "build": "rimraf lib && NODE_ENV=production gulp build",
    "lint": "fecs",
    "docs": "esdoc -c esdoc.json",
    "setup": "rimraf asset && NODE_ENV=production webpack --config=tools/webpack.dll.js",
    "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
    "test-ci": "NODE_ENV=test karma start ./test/karma.ci.conf.js",
    "coveralls": "cat ./test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.24.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs": "^0.8.6",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-expect-jsx": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "nib": "^1.1.2",
    "numen": "^0.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.16.5"
